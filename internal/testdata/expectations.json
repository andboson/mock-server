[
  {
    "method": "GET",
    "path": "/api/hello",
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "mock": "{\"message\": \"Hello, World!\"}"
  },
  {
    "method": "GET",
    "path": "/api/users/\\d+",
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "mock": "{\"id\": 123, \"name\": \"John Doe\"}"
  },
  {
    "method": "POST",
    "path": "/api/submit",
    "request": ".*important.*",
    "status": 201,
    "headers": {
      "Content-Type": "application/json"
    },
    "mock": "{\"status\": \"created\"}"
  },
  {
    "method": "GET",
    "path": "/api/data.xml",
    "status": 200,
    "headers": {
      "Content-Type": "application/xml"
    },
    "mock": "<root><data>value</data></root>"
  },
  {
      "method": "POST",
      "path": "/api/.*",
      "status": 204,
      "mock": "{\"foo\": \"bar\"}"
  }
